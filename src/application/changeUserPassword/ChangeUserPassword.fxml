<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="235.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.changeUserPassword.ChangeUserPasswordController">
   <children>
      <TitledPane alignment="CENTER" animated="false" collapsible="false" layoutY="-1.0" prefHeight="235.0" prefWidth="300.0" text="Change The Password">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <PasswordField fx:id="newPass" layoutX="65.0" layoutY="28.0" prefHeight="38.0" prefWidth="168.0" promptText="New Password" />
                  <PasswordField fx:id="confirmNewPass" layoutX="65.0" layoutY="80.0" prefHeight="38.0" prefWidth="168.0" promptText="Confirm New Password" />
                  <Button fx:id="close" layoutX="31.0" layoutY="139.0" mnemonicParsing="false" onAction="#closeButton" prefWidth="68.0" text="Close" />
                  <Button fx:id="save" layoutX="206.0" layoutY="139.0" mnemonicParsing="false" onAction="#saveButton" prefWidth="68.0" text="Save" />
               </children></AnchorPane>
        </content>
         <font>
            <Font size="24.0" />
         </font>
      </TitledPane>
   </children>
</AnchorPane>
