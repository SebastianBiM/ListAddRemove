<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.generatePassword.GeneratePasswordController">
   <children>
      <TitledPane alignment="CENTER" animated="false" collapsible="false" layoutX="-1.0" prefHeight="300.0" prefWidth="300.0" text="Generate New Password" textAlignment="CENTER">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="251.0" prefWidth="300.0">
               <children>
                  <TextField fx:id="uniqW" layoutX="55.0" layoutY="26.0" prefHeight="33.0" prefWidth="175.0" promptText="Write Your Unique Word" />
                  <Button fx:id="generatePass" alignment="CENTER" contentDisplay="BOTTOM" layoutX="62.0" layoutY="77.0" mnemonicParsing="false" onAction="#generateNewPassword" text="Generate New Password" />
                  <TextArea fx:id="newPassArea" layoutX="43.0" layoutY="134.0" prefHeight="45.0" prefWidth="200.0" />
                  <Button fx:id="finish" layoutX="212.0" layoutY="198.0" mnemonicParsing="false" onAction="#finishButton" text="Finish" />
                  <Button fx:id="close" layoutX="24.0" layoutY="198.0" mnemonicParsing="false" onAction="#closeButton" prefHeight="33.0" prefWidth="63.0" text="Close" />
               </children>
            </AnchorPane>
        </content>
         <font>
            <Font size="24.0" />
         </font>
      </TitledPane>
   </children>
</AnchorPane>
